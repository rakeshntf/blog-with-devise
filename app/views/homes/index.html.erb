<%- model_class = Home -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead> 
   
  </thead>
  <tbody>
    <% @homes.each do |home| %>
    <tr>

<%= link_to image_tag(home.avatar.url(:thumb)),
 {:controller=>"homes",:action=>"show",:id=>home.id} %> <BR/>
  <%= home.name%><BR/><br/>
 


<%#= link_to image_tag(home.avatar.url(:thumb)),
